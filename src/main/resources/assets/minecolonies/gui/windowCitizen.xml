<window inherit="minecolonies:gui/windowBase.xml"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="file:../../../../java/com/blockout/blockOut.xsd">
    <label id="title" size="100% 11" pos="0 9"
           textalign="TopMiddle" color="black"
           label="$(com.minecolonies.gui.citizen.skills)" />

    <box padding="3" size="100 61" pos="30 30">
        <group size="100% 100%">
            <label id="strength" size="100% 11" color="black" />
            <label id="stamina" size="100% 11" color="black" />
            <label id="wisdom" size="100% 11" color="black" />
            <label id="intelligence" size="100% 11" color="black" />
            <label id="charisma" size="100% 11" color="black" />
        </group>
    </box>

    <button id="inventory"
            align="BottomMiddle" size="116 20" pos="0 13"
            label="$(container.inventory)" />
</window>
